<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	 <class name="com.uws.model.ProvideSupplyModel" table="PROVIDE_SUPPLY">
	 	 <id name="id" type="java.lang.String">
            <column name="ID" length="32" />
            <generator class="uuid" />
        </id>
        
		<property name="USER_ID"  	type="java.lang.String" column="USER_ID"/>
		<property name="NAME" 	type="java.lang.String" column="NAME"/>
		<property name="DETAIL"  	type="java.lang.String" column="DETAIL"/>
		<property name="START_TIME" 	type="java.lang.String" column="START_TIME"/>
		<property name="LINKMAN" 	type="java.lang.String" column="LINKMAN"/>
		<property name="PHONE"  	type="java.lang.String" column="PHONE"/>
		<property name="TEL"  	type="java.lang.String" column="TEL"/>
		<property name="MAIL" 	type="java.lang.String" column="MAIL"/>
		<property name="STATUS" 	type="java.lang.String" column="STATUS"/>
		
		<property name="createTime" 	type="timestamp" column="CREATE_TIME"/>
		<property name="updateTime" 	type="timestamp" column="UPDATE_TIME"/>
	 </class>

</hibernate-mapping>